-family: \"regular\", serif;\r\n    background-color: $color1;\r\n    font-size: $fsizeSmall;\r\n    h1{\r\n        font-family: \"title\", serif;\r\n        text-align: center;\r\n        padding-bottom: 30px;\r\n    }\r\n    a{\r\n        color: $color3;\r\n        text-decoration: none;\r\n        cursor: pointer;\r\n        .Anim{\r\n            width: 25px;\r\n            height: 35px;\r\n            float: right;\r\n            transition: 0s;\r\n        }\r\n    }\r\n    a:hover{\r\n        color: $color2;\r\n        transition: color 0.5s;\r\n    }\r\n    \r\n    .NewsBanner, .GalleryBanner, .MainBanner{\r\n        font-family: \"title\", serif;\r\n        position: absolute;\r\n        top: 0%;\r\n        left: 0%;\r\n        margin-right: 0%;\r\n        padding: 30px 2% 10px 2%;\r\n        width: 96%;\r\n        min-width: 700px;\r\n        height: 150px;\r\n        background-color: $color2;\r\n        background: linear-gradient(0deg, $color2 0%, $color2_5 100%);\r\n        display: grid;\r\n        grid-template-columns: 250px 75%;\r\n        .dropdown{\r\n            width: 250px;\r\n            text-align: right;\r\n            .linkDown{\r\n                display: grid;\r\n                grid-template-rows: 25% 75%;\r\n                margin-left: auto;\r\n                margin-right: 0;\r\n                height: 50px;\r\n                border-radius: 10px;\r\n                \r\n            }\r\n            .dropdown-content{\r\n                height: 300px;\r\n                display: none;\r\n                grid-template-rows: auto;\r\n            }\r\n        }\r\n        .logo{\r\n            padding-left: 20px;\r\n            min-width: 250px;\r\n            img{\r\n                width: 200px;\r\n            }\r\n        }\r\n        a{\r\n            color: black;\r\n            text-decoration: none;\r\n        }\r\n        a:hover{\r\n            color: white;\r\n            transition: 0.3s;\r\n        }\r\n        .link{\r\n            width: 250px;\r\n            text-align: right;   \r\n        }\r\n        .linkBack{\r\n            margin-top: 100px;\r\n            margin-right: 0px;\r\n            text-align: right;  \r\n        }\r\n        .links{\r\n            margin-top: 100px;\r\n            display: flex;\r\n            justify-content: space-evenly;\r\n        } \r\n    }\r\n\r\n    .NewsMain, .galleryMain, .IndexMain{\r\n        margin-top: 0px;\r\n        padding: 50px 100px 50px 100px;\r\n        text-align: justify;\r\n        .newsImage, .mainImage{\r\n            img{\r\n                width: 300px;\r\n            } \r\n        }\r\n        .newsImage:hover, .mainImage:hover{\r\n            transform: scale(1.1);\r\n            transition: 0.3s;\r\n            cursor: pointer;\r\n        }\r\n        .mainImage{\r\n            float: left;\r\n            padding: 10px 30px 10px 10px;\r\n        }\r\n        div#image1, div#image3{\r\n            float: left;\r\n            padding: 10px 30px 10px 10px;\r\n        }\r\n        div#image2{\r\n            float: right;\r\n            padding: 10px 10px 10px 30px;\r\n        }  \r\n    }\r\n    .galleryMain{\r\n        display: flex;\r\n        flex-wrap: wrap;\r\n        justify-content: space-evenly;\r\n        \r\n        .photo{\r\n            padding: 10px;\r\n            img{\r\n                width: 300px;\r\n            }\r\n        }\r\n        .photo:hover{\r\n            transform: scale(1.1);\r\n            transition: 0.3s;\r\n            cursor: pointer;\r\n        }\r\n    }\r\n    .IndexMain{\r\n        display: grid;\r\n        .new{\r\n            padding-top: 0px; \r\n            padding-bottom: 100px;\r\n        }\r\n        time{\r\n            font-style: italic;\r\n        }\r\n        h1{\r\n            font-size: $fsizeMed;\r\n            color: black;\r\n        }\r\n        h1:hover{\r\n            color: $color3;\r\n            transition: 0.3s;\r\n        }\r\n        .line{\r\n            background-color: $color2;\r\n            width: 90%;\r\n            height: 7px;\r\n            margin-bottom: 100px;\r\n            margin-left: auto;\r\n            margin-right: auto;\r\n        }\r\n    }\r\n    .NewsMain{\r\n        margin-top: 150px;\r\n        h1{\r\n            font-size: $fsizeBig;\r\n        } \r\n    }\r\n    footer{\r\n        margin: 0% 0% auto auto;\r\n        text-align: right;\r\n        font-size: 15px;\r\n    }\r\n    .openImg{\r\n        background-color: rgba(126, 126, 126, 0.85);\r\n        position: fixed;\r\n        display: grid;\r\n        grid-template-columns: 15% 70% 15%;\r\n        grid-template-rows: 5% 95%;\r\n        top: 0%;\r\n        left: 0%;\r\n        width: 100%;\r\n        height: 100%;\r\n        img{\r\n            max-width: 100%;\r\n            max-height: 90%;\r\n            display: flex;\r\n            margin: auto;\r\n            align-items: center;\r\n            justify-content: center;\r\n            grid-column: 2;\r\n            grid-row: 2;\r\n        }\r\n        .exitBtn, .prevBtn, .nextBtn{\r\n            cursor: pointer;\r\n            color: black;\r\n            font-size: 50px;\r\n        }\r\n        .exitBtn:hover, .prevBtn:hover, .nextBtn:hover{\r\n            color: white;\r\n            transition: 0.3s;\r\n        }\r\n        .exitBtn{\r\n            grid-row: 1;\r\n            grid-column: 1 / 4;\r\n            padding-top: 40px;\r\n            padding-right: 50px;\r\n            text-align: right;\r\n        }\r\n        .prevBtn, .nextBtn{\r\n            grid-row: 2;\r\n            display: flex;\r\n            margin: auto;\r\n            align-items: center;\r\n            justify-content: center;\r\n        }\r\n        .prevBtn{\r\n            grid-column: 1;\r\n        }\r\n        .nextBtn{\r\n            grid-column: 3;\r\n        }\r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"f1a85aafaf91f45cafa2e529a850ec46","cacheData":{"env":{}}}